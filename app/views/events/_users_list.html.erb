<% if @users.present? %>
  <% @users.each do |user|%>
    <div class="supporters2">
      <% if user.user_pictures.present? && user.user_pictures.last.image.present? %><%= image_tag user.user_pictures.last.image.url(:medium), alt: '', style: 'height: 80px;' %><% else %><img src="/assets/noImg.jpg"/><% end %>
      <% if user.first_name.present? || user.first_name.present? %><h2><%= user.full_name %><% end %>
      <% if user.city.present? || user.state.present? %><h2><span><%= user.full_address %></span></h2><% end %>
      <h2><span>300 Gigs 700 Hours</span></h2>
    </div>
  <% end %>
<% else %>
 <div class ="no-result"> <h2> No results matching your search criteria </h2> </div>
<% end %>
<script>
	$(window).load(function() {
		$('.searchRight').text("No search results");
	});
</script>

<style>
	 .no-result{
		 padding-top: 110px;
	 }
</style>