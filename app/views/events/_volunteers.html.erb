<h1 class="title-event">Top 6 Glis Volunteers</h1>
<table class="volunteer_list">
  <tbody>
  <% @volunteers.in_groups_of(2, false) do |volunteer| %>
    <tr>
      <% for vol in volunteer %>
        <td><%= image_tag vol.image.url(:medium) %></td>
      <% end %>
    </tr>
  <% end %>
  </tbody>
</table>
