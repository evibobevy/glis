<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="MobileOptimized" content="320">
  <meta name="format-detection" content="telephone=no">
  <meta name="viewport" content="user-scalable=1.0,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0">
  <title>:: Glis ::</title>
  <%= stylesheet_link_tag 'application' %>
  <!--end  Event on -off button -->
  <%= stylesheet_link_tag 'flaticon' %>
  <%= stylesheet_link_tag 'highlight' %>
  <%= stylesheet_link_tag 'bootstrap-switch' %>
  <%= stylesheet_link_tag 'jquery-ui' %>
  <%= stylesheet_link_tag 'jquery.timepicker.min' %>
  <!--<link rel="stylesheet" type="text/css" href="/assets/flaticon.css">-->
  <!--<link href="/assets/highlight.css" rel="stylesheet">-->
  <!--<link href="/assets/bootstrap-switch.css" rel="stylesheet">-->
  <!--<link rel="stylesheet" href="/assets/jquery-ui.css">-->
  <!--<link rel="stylesheet" href="/assets/jquery.timepicker.min.css">-->
  <%= javascript_include_tag 'application' %>
  <%= javascript_include_tag 'jquery.timepicker.min' %>
  <!--<script src="/assets/jquery.timepicker.min.js"></script>-->
</head>
<body>
<!--Main Wrapper Starts Here-->
<div id="wrapper">
  <!--Header Starts Here-->
    <%= render 'shared/header' %>
  <!--Header ends here-->

  <!--Middle content starts here-->
  <section id="midd-container">
    <% if user_signed_in? %>
      <%= render 'shared/volunteer_section' if action_name != 'profile_settings' %>
    <% else %>
      <%= render 'shared/social_network' %>
    <% end %>
    <%= yield %>
  </section>
  <!--Middle content ends here-->
  <!--Footer Starts here-->
    <%= render 'shared/footer' %>
  <!--Footer ends here-->
</div>
  <!--Main Wrapper Ends Here-->
</body>
</html>
